<h1>New User</h1>
<form action="<%= users_url %>" method="POST">

  <label>
    Username:
    <input type="text"
      name="user[user_name]"
      value="<%= @user.user_name %>"><br>
  </label>

  <label>
    Password:
    <input type="password"
      name="user[password]"
      value="<%= @user.password %>"><br>
  </label>

  <button>Submit!</button>
</form>
